  build-depends:       base >=4.6 && <4.7,
                       containers -any,
                       lens -any,
                       linear -any,
                       attoparsec -any,
                       bytestring -any,
                       bytestring-builder -any,
                       cereal -any
  hs-source-dirs:      src
  default-language:    Haskell2010
  ghc-options:         -Wall
